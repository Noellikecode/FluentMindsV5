{
  "expo": {
    "name": "Fluent Minds",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.noelthomas.fluentminds",
      "buildNumber": "31",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSSpeechRecognitionUsageDescription": "This app uses speech recognition to analyze your vocal patterns and provide feedback in activities.",
        "NSCameraUsageDescription": "This app may need camera access for features like recording videos or capturing images to support speech and activity exercises.",
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-av",
        {
          "microphonePermission": "This app uses the microphone to allow you to record your voice for rhythm, speech, and storytelling exercises."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "acfe0380-3680-4a40-ac77-50e1b1750c3c"
      }
    },
    "android": {
      "package": "com.noelthomas.fluentminds",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "owner": "noel124",
    "updates": {
      "url": "https://u.expo.dev/acfe0380-3680-4a40-ac77-50e1b1750c3c"
    }
  }
}
